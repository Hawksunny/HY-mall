{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": ["src/*"],
      "assets/*": ["src/assets/*"],
      "components/*": ["src/components/*"],
      "network/*": ["src/network/*"]
    },
    "lib": ["esnext", "dom", "dom.iterable", "scripthost"],
    "jsx": "preserve"
  }
}
